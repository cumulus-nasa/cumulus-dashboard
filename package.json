    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "assert": "^1.4.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "brace": "^0.9.1",
    "classnames": "^2.2.5",
    "compare-versions": "^3.4.0",
    "dagre-d3": "^0.6.4",
    "deepmerge": "^1.3.2",
    "global": "^4.3.1",
    "history": "3.0.0",
    "jquery": "3.4.0",
    "jsonwebtoken": "^8.3.0",
    "localized-strings": "0.0.11",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.findkey": "^4.6.0",
    "lodash.frompairs": "^4.0.1",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isnil": "^4.0.0",
    "lodash.map": "^4.6.0",
    "lodash.omitby": "^4.6.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.17.1",
    "numeral": "^2.0.4",
    "object-path": "^0.11.3",
    "paginator": "^1.0.0",
    "popper.js": "^1.14.7",
    "prop-types": "^15.5.10",
    "react": "^16.10.2",
    "react-ace": "^6.2.0",
    "react-autocomplete": "^1.7.1",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-collapsible": "^2.0.4",
    "react-dom": "^16.6.3",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
