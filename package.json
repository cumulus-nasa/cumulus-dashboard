{
  "name": "cumulus-dashboard",
  "version": "1.6.1",
  "description": "A dashboard for Cumulus API",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus-dashboard"
  },
  "author": {
    "name": "Development Seed",
    "url": "https://developmentseed.org"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nasa/cumulus-dashboard/issues"
  },
  "homepage": "https://github.com/nasa/cumulus-dashboard",
  "scripts": {
    "audit-ci": "audit-ci --high -p npm",
    "serve": "gulp serve",
    "build": "gulp",
    "lint": "eslint app/src/js/ --ext .js",
    "start": "npm run serve",
    "cypress": "CYPRESS_TESTING=true NODE_ENV=test cypress open",
    "cypress-ci": "CYPRESS_TESTING=true NODE_ENV=test cypress run --spec 'cypress/integration/*' --config video=false",
    "validation": "ava cypress/validation-tests/*.js",
    "test": "npm run lint && ava",
    "seed-database": "LOCALSTACK_HOST=localhost LOCAL_ES_HOST=localhost NODE_ENV=test node ./localAPI/runSeedDatabase.js",
    "start-localapi": "docker-compose -f ./localAPI/docker-compose.yml up -d",
    "stop-localapi": "docker-compose -f ./localAPI/docker-compose.yml down",
    "start-dashboard": "docker-compose -f ./localAPI/docker-compose.yml -f ./localAPI/docker-compose-dashboard.yml up -d",
    "stop-dashboard": "docker-compose -f ./localAPI/docker-compose.yml -f ./localAPI/docker-compose-dashboard.yml down",
    "e2e-tests": "docker-compose -f ./localAPI/docker-compose.yml -f ./localAPI/docker-compose-dashboard.yml -f ./localAPI/docker-compose-cypress.yml up --exit-code-from=e2e --abort-on-container-exit",
    "view-docker-logs": "docker-compose -f ./localAPI/docker-compose.yml -f ./localAPI/docker-compose-dashboard.yml logs -f "
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/fake-api/**/*",
      "!test/fixtures/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ],
      "require-globify",
      "envify"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@cumulus/api": "^1.17.0",
    "@cypress/browserify-preprocessor": "~2.1.1",
    "audit-ci": "1.3.0",
    "ava": "^2.4.0",
    "ava-http": "^1.0.0",
    "babel-core": "^7.0.0-bridge",
    "babelify": "^9.0.0",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.0",
    "cached-path-relative": "1.0.2",
    "cls-bluebird": "^2.1.0",
    "cypress": "^3.8.2",
    "cypress-failed-log": "^2.5.0",
    "cypress-pipe": "^1.6.0",
    "del": "^2.2.0",
    "envify": "^3.4.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^2.1.2",
    "eslint-plugin-standard": "^1.3.2",
    "express": "^4.16.3",
    "fs-extra": "^5.0.0",
    "gulp": "^4.0.0",
    "gulp-cache": "^0.4.4",
    "gulp-csso": "^2.0.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^5.0.0",
    "gulp-load-plugins": "^1.2.2",
    "gulp-plumber": "^1.1.0",
    "gulp-preprocess": "^3.0.1",
    "gulp-rev": "^7.0.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^3",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify-es": "^1.0.4",
    "gulp-useref": "^4.0.0",
    "gulp-util": "^3.0.7",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "nock": "^10.0.4",
    "node-bourbon": "^4.2.8",
    "node-notifier": "^4.5.0",
    "react-test-renderer": "^16.6.3",
    "redux-mock-store": "^1.5.3",
    "redux-test-utils": "^0.2.2",
    "require-globify": "^1.3.0",
    "sinon": "^7.5.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
